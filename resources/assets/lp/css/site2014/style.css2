
/* geral */
body{font-family: proxima_nova_rgregular; background: url(../../img/site2014/bg-site.jpg);}

/* Erro Validação */
.error { color: #FF0000 !important;}
input.error { background: #FFD1D1 !important; border: 1px solid #FF0000 !important;}
select.error { background: #FFD1D1 !important; border: 1px solid #FF0000 !important;}
textarea.error { background: #FFD1D1 !important; border: 1px solid #FF0000 !important;}
label.error { display: none !important;}
div.custom_error{ background: #D6E3D0 !important;}
.custom_error { color: red !important;}
/* Erro Validação */

/* message boxes
------------------------------------------------------------------------- */
div.msg {
  margin-bottom: 0;
  padding: 10px 10px 10px 37px;
  cursor: pointer;
}
div.success {
  border: 1px solid #6cd858;
  background: #e3ffde url(../../img/admin/icons/tick.png) 10px 13px no-repeat;
}
div.error {
  border: 1px solid #ffa2aa;
  background: #fae8e8 url(../../img/admin/icons/exclamation-red.png) 10px 13px no-repeat;
}
div.warning {
  border: 1px solid #d6d61f;
  background: #ffffd3 url(../../img/admin/icons/exclamation.png) 10px 13px no-repeat;
}
div.information {
  border: 1px solid #b4dbe8;
  background: #e3f2f7 url(../../img/admin/icons/information-balloon.png) 10px 13px no-repeat;
}

.setFlash_site div.success {
  border: 1px solid #333;
  background: #333 url(../../img/test-pass-icon.png) 10px 18px no-repeat;
  position: relative; z-index:99; font: 24px proxima_nova_rgbold; color: #fff; padding: 15px 10px 15px 37px;

}
.setFlash_site div.error {
  border: 1px solid #333;
  background: #333 url(../../img/test-fail-icon.png) 10px 19px no-repeat;
  position: relative; z-index:99; font: 24px proxima_nova_rgbold; color: #fff !important; padding: 15px 10px 15px 37px;
}

.setFlash_site div.warning {
  border: 1px solid #333;
  background: #333 url(../../img/test-fail-icon.png) 10px 19px no-repeat;
  position: relative; z-index:99; font: 24px proxima_nova_rgbold; color: #fff !important; padding: 15px 10px 15px 37px;
}


